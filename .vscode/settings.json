{
    "spellright.language": [
        "en"
    ],
    "spellright.documentTypes": [
        "markdown",
        "latex",
        "plaintext",
        "nim",
        "nimble",
        "nims"
    ],
    "spellright.parserByClass": {
        "nim": {
            "parser": "code"
        },
        "nimble": {
            "parser": "code"
        },
        "nims": {
            "parser": "code"
        }
    },
    "spellright.notificationClass": "information",
    "spellright.ignoreFiles": [
        "**/.gitignore",
        "**/.spellignore",
        "outputExpected.txt",
        "outputGotten.txt",
        "megatest.nim",
        "nimcache/*"
    ]
}
